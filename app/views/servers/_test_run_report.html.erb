<div class="test-run-report" data-server-id="<%=server.id %>">
  <div class="row server-fhir-specification section-data">
    <%= render partial: "components/server_summary", locals: {server: server, noHeader: true, extended: true}%>
  </div>
  <div class="server-history server-history-loading">
  </div>
  <!-- START: badges -->
  <% if @server.badges %>
  <div class="row" style="height:50px;">
    <% for badge in Badge.all %>
    <div class="col-sm-3">
      <% if @server.badges.include? badge.id %>
      <span style="background-color:orange;padding:10px;margin:10px" title="badge description">
        <%= badge.id %>
      </span>
      <% end %>
    </div>
    <% end %>
  </div>
  <% end %>
  <!-- END: badges -->
  <div class="failures row">
    <div class="common-failures col-sm-6">
      <div class="failures-title">
        Recent Common Failures
      </div>
      <div>
        <br><br>
        Loading
      </div>
    </div>
    <div class="recent-runs col-sm-6">
      <div class="recent-runs-title">
        Recent Test Runs
      </div>
      <div>
        <br><br>
        Loading
      </div>
    
    </div>
  </div>
</div>
